{
  "name": "rss-to-bluesky-social",
  "version": "0.0.0",
  "description": "",
  "main": "build/index.js",
  "license": "Apache-2.0",
  "keywords": [],
  "scripts": {
    "clean": "rimraf build dist",
    "lint": "pnpm run license && eslint --fix --no-error-on-unmatched-pattern src/ test/",
    "bundle": "rollup --no-treeshake -c rollup.config.mjs",
    "create": "mkdir ./dist && clasp create --title rss-to-bluesky-social --type standard --rootDir ./dist && mv ./dist/.clasp.json ./.clasp.json",
    "build": "pnpm run clean && pnpm run bundle && ncp appsscript.json dist/appsscript.json",
    "login": "clasp login",
    "license": "license-check-and-add add -f license-config.json",
    "test": "jest test/ --passWithNoTests --detectOpenHandles",
    "deploy": "pnpm run lint && pnpm run test && pnpm run build && clasp push -f"
  },
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "@google/clasp": "^2.4.2",
    "@types/google-apps-script": "^1.0.68",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "gts": "^5.0.0",
    "jest": "^29.6.2",
    "license-check-and-add": "^4.0.5",
    "ncp": "^2.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.5",
    "rollup": "^3.27.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-license": "^3.0.1",
    "rollup-plugin-typescript2": "^0.35.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}
